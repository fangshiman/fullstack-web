{
  "name": "fullstack-test",
  "version": "0.1.0",
  "description": "A Simple Full Stack Web Project",
  "main": "index.js",
  "author": "Fangshiman",
  "license": "MIT",
  "scripts": {
    "start": "node dist/src/server.js",
    "server": "nodemon src/server.ts",
    "build": "tsc -p .",
    "client": "yarn --cwd client/ start",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --runInBand --detectOpenHandles --watchAll"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/node": "14.0.5",
    "concurrently": "5.2.0",
    "nodemon": "2.0.4",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "@types/async": "3.2.3",
    "@types/cron": "1.7.2",
    "@types/jest": "25.2.3",
    "@types/mongoose": "5.7.21",
    "@types/morgan": "1.9.0",
    "@types/sinon": "9.0.4",
    "@types/supertest": "2.0.12",
    "cron": "1.8.2",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "jest": "28.0.0",
    "mongoose": "5.9.15",
    "morgan": "1.10.0",
    "sinon": "9.0.2",
    "supertest": "6.3.1",
    "ts-jest": "28.0.0"
  }
}
